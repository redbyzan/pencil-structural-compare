{
  "name": "@pencil-structural/compare",
  "version": "0.1.0",
  "description": "Structural comparison engine for Pencil designs and code",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "structural-compare": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./normalizers": {
      "types": "./dist/normalizers/index.d.ts",
      "import": "./dist/normalizers/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./discovery": {
      "types": "./dist/discovery/index.d.ts",
      "import": "./dist/discovery/index.js"
    },
    "./processor": {
      "types": "./dist/processor/index.d.ts",
      "import": "./dist/processor/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "pencil",
    "structural-comparison",
    "design-system",
    "code-verification",
    "cli",
    "configuration"
  ],
  "author": "redbyzan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/redbyzan/pencil-structural-compare.git"
  },
  "bugs": {
    "url": "https://github.com/redbyzan/pencil-structural-compare/issues"
  },
  "homepage": "https://github.com/redbyzan/pencil-structural-compare#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "globby": "^14.0.0",
    "micromatch": "^4.0.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.10",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
